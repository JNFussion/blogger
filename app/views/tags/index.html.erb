<h1>All tags<h1>

<ul id='tags'>
  <% @tags.each do |tag| %>
    <li><%= link_to tag.name, tag_path(tag), class: 'tag_name', id: "tag_#{tag.id}"%>
    <% if logged_in? %>
      <%= link_to "delete", tag_path(tag) , method: :delete, data: { confirm: "Really delete the tag?"} , class: 'delete_link'%></li>
    <%end%>
  <%end%>
</ul>

<%= link_to "Back to all articles", articles_path, class: 'back_link'%>